{
  "name": "rinda-callops",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "generate-api": "tsx scripts/generate-api-client.ts",
    "generate-api:watch": "tsx scripts/generate-api-client.ts && echo 'Watching for server changes...' && while true; do sleep 5; curl -s http://localhost:8000/openapi.json > /tmp/openapi-new.json && if ! cmp -s /tmp/openapi-old.json /tmp/openapi-new.json 2>/dev/null; then echo 'OpenAPI changed, regenerating...'; tsx scripts/generate-api-client.ts; cp /tmp/openapi-new.json /tmp/openapi-old.json; fi; done"
  },
